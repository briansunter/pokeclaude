{
  "name": "pokemon-pocket-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for querying Pokemon Pocket TCG cards with DuckDB",
  "type": "module",
  "bin": {
    "pokemon-pocket-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:manual": "bun run test-server.ts"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.4.0-r.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
